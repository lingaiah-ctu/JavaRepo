public class Calculation {

	public static void main(String[] args)
	{
	  int a=100,b=200,c;
	  c=a+b;
	  System.out.println("Addition of a and b : "+c);
	  c=a*b;
	  System.out.println("multiplication of a and b : "+c);
	  c=b%a;
	  System.out.println("Division of b and a : "+c);
	  
